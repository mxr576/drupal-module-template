{
    "name": "pronovix/drupal-module-template",
    "description": "Pronovix Drupal module template.",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal/core": "^8.7",
        "php": "^7.1"
    },
    "require-dev": {
        "behat/mink": "dev-master#153e4bb",
        "drupal/drupal-extension": "^3.4.0",
        "instaclick/php-webdriver": "^1.4.5",
        "phpunit/phpunit": "^6.5",
        "webflo/drupal-core-require-dev": "^8.7"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Drupal\\drupal_module_template\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\drupal_module_template\\": "./tests/"
        }
    }
}
